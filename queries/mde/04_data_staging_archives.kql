// Collection/Staging â€” archive tools or bulk file activity
DeviceProcessEvents
| where DeviceName =~ "azuki-sl"
| where Timestamp > ago(30d)
| where FileName in~ ("7z.exe","7za.exe","rar.exe","winrar.exe","zip.exe","tar.exe")
| project Timestamp, AccountName, FileName, ProcessCommandLine, InitiatingProcessFileName, SHA256
| order by Timestamp asc